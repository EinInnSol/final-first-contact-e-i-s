{
  "name": "first-contact-eis",
  "version": "1.0.0",
  "description": "AI-Powered Early Intervention System for Long Beach Civic Services",
  "private": true,
  "workspaces": [
    "frontend/client",
    "frontend/caseworker",
    "frontend/city",
    "frontend/kiosk",
    "frontend/admin",
    "frontend/shared"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:caseworker\" \"npm run dev:city\" \"npm run dev:kiosk\" \"npm run dev:admin\"",
    "dev:client": "cd frontend/client && npm run dev",
    "dev:caseworker": "cd frontend/caseworker && npm run dev",
    "dev:city": "cd frontend/city && npm run dev",
    "dev:kiosk": "cd frontend/kiosk && npm run dev",
    "dev:admin": "cd frontend/admin && npm run dev",
    "build": "npm run build:client && npm run build:caseworker && npm run build:city && npm run build:kiosk && npm run build:admin",
    "build:client": "cd frontend/client && npm run build",
    "build:caseworker": "cd frontend/caseworker && npm run build",
    "build:city": "cd frontend/city && npm run build",
    "build:kiosk": "cd frontend/kiosk && npm run build",
    "build:admin": "cd frontend/admin && npm run build",
    "start": "concurrently \"npm run start:client\" \"npm run start:caseworker\" \"npm run start:city\" \"npm run start:kiosk\" \"npm run start:admin\"",
    "start:client": "cd frontend/client && npm start",
    "start:caseworker": "cd frontend/caseworker && npm start",
    "start:city": "cd frontend/city && npm start",
    "start:kiosk": "cd frontend/kiosk && npm start",
    "start:admin": "cd frontend/admin && npm start",
    "test": "npm run test:client && npm run test:caseworker && npm run test:city && npm run test:kiosk && npm run test:admin",
    "test:client": "cd frontend/client && npm test",
    "test:caseworker": "cd frontend/caseworker && npm test",
    "test:city": "cd frontend/city && npm test",
    "test:kiosk": "cd frontend/kiosk && npm test",
    "test:admin": "cd frontend/admin && npm test",
    "lint": "npm run lint:client && npm run lint:caseworker && npm run lint:city && npm run lint:kiosk && npm run lint:admin",
    "lint:client": "cd frontend/client && npm run lint",
    "lint:caseworker": "cd frontend/caseworker && npm run lint",
    "lint:city": "cd frontend/city && npm run lint",
    "lint:kiosk": "cd frontend/kiosk && npm run lint",
    "lint:admin": "cd frontend/admin && npm run lint",
    "type-check": "npm run type-check:client && npm run type-check:caseworker && npm run type-check:city && npm run type-check:kiosk && npm run type-check:admin",
    "type-check:client": "cd frontend/client && npm run type-check",
    "type-check:caseworker": "cd frontend/caseworker && npm run type-check",
    "type-check:city": "cd frontend/city && npm run type-check",
    "type-check:kiosk": "cd frontend/kiosk && npm run type-check",
    "type-check:admin": "cd frontend/admin && npm run type-check",
    "clean": "npm run clean:client && npm run clean:caseworker && npm run clean:city && npm run clean:kiosk && npm run clean:admin",
    "clean:client": "cd frontend/client && rm -rf .next node_modules",
    "clean:caseworker": "cd frontend/caseworker && rm -rf .next node_modules",
    "clean:city": "cd frontend/city && rm -rf .next node_modules",
    "clean:kiosk": "cd frontend/kiosk && rm -rf .next node_modules",
    "clean:admin": "cd frontend/admin && rm -rf .next node_modules",
    "install:all": "npm install && npm run install:client && npm run install:caseworker && npm run install:city && npm run install:kiosk && npm run install:admin",
    "install:client": "cd frontend/client && npm install",
    "install:caseworker": "cd frontend/caseworker && npm install",
    "install:city": "cd frontend/city && npm install",
    "install:kiosk": "cd frontend/kiosk && npm install",
    "install:admin": "cd frontend/admin && npm install"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "concurrently": "^8.2.2",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "civic-services",
    "ai-powered",
    "early-intervention",
    "case-management",
    "compliance",
    "accessibility",
    "multi-language",
    "long-beach",
    "government",
    "social-services"
  ],
  "author": "City of Long Beach - First Contact EIS Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/longbeach/first-contact-eis.git"
  },
  "bugs": {
    "url": "https://github.com/longbeach/first-contact-eis/issues"
  },
  "homepage": "https://firstcontact-eis.org"
}
